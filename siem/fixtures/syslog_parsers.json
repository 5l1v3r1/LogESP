  "model": "siem.logeventparser",
  "fields": {
    "name": "syslog",
    "desc": "Standard syslog parser",
    "match_regex": "^([A-Z][a-z]{2}\\s+\\d{1,2}\\s+\\d{2}:\\d{2}:\\d{2})\\s+(\\S+)\\s+([^\\[\\] ]+)\\[?(\\d*)\\]?: (.*)",
    "backup_match_regex": null,
    "fields": "date_stamp,log_source,source_process,source_pid,message",
    "backup_fields": null
  }
},
