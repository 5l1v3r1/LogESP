    "match_regex": "User\\s+CLI\\s+session\\s+for\\s+user\\s+(\\S+)\\s+over\\s+(\\S+)\\s*,\\s+source\\s+(\\S+)\\s+destination\\s+(\\S+)",
    "fields": "target_user,protocol,source_host,dest_host"
  }
},
{
  "model": "siem.parsehelper",
  "fields": {
    "name": "ssh_pubkey_auth",
