    "match_regex": "New\\s+(\\S+)\\s+connection,\\s+source\\s+(\\S+)\\s+destination\\s+(\\S+)",
    "fields": "protocol,source_host,dest_host"
  }
},
{
  "model": "siem.parsehelper",
  "fields": {
